<?xml version="1.0" encoding="UTF-8"?>
<skin name="ASCII Keyboard Controller" author="zoggins" type="gamecube">

  	<!-- Load Controller images - Menu name, file path + name, read in order -->
  	<!-- Any new image/skin you make must have it's own entry in here! -->
    <background name="Default" image="keyboard.png" />

	<!-- Face buttons. No need to specify width/height unless you want to transform image size -->
    <button name="a" image="circle2.png" x="1043" y="126" height="50" width="50" />
    <button name="b" image="circle.png" x="1004" y="158" height="30" width="30" />
    <button name="x" image="circle2.png" x="1104" y="125" height="25" width="25" />
    <button name="y" image="circle2.png" x="1043" y="95" height="25" width="25" />
    <button name="z" image="circle2.png" x="1058" y="57" height="25" width="25" />

    <button name="start" image="circle2.png" x="169" y="152" height="20" width="20" />

	<!-- L/R triggers and the pressure bar associated with them -->
    <button name="r" image="RL-full.png" x="1117" y="25" width="8" height="8" />
    <analog name="trig_r" image="R-bar.png" x="1020" y="25" width="100" height="8"
            direction="right" reverse="false" />

    <button name="l" image="RL-full.png" x="17" y="25" width="8" height="8" />
    <analog name="trig_l" image="L-bar.png" x="17" y="25" width="100" height="8"
            direction="left" reverse="false" />

	<!-- D-Pad -->
    <button name="up" image="circle2.png" x="110" y="209" height="20" width="20" />
